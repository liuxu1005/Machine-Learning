#!user/bin/perl



open(my $GP,'| gnuplot');

print {$GP} << "__GNUPLOT__";


set ylabel "Accuracy ";
set xlabel "folds";
set xrang[0:9]; 
set xtics ("1" 0, "2" 1, "3" 2, "4" 3, "5" 4, "6" 5, "7" 6, "8" 7, "9" 8, "10" 9);
set terminal png;

set output 'm1.png';
plot  "m1_ibm" u 7 w lp title 'm0'; 
__GNUPLOT__

close $GP;
